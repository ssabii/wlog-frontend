{
  "name": "wlog-frontend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/ssabii/wlog-frontend",
  "author": "ssabi",
  "license": "UNLICENSED",
  "scripts": {
    "start": "cross-env stage=$stage webpack-dev-server --config webpack.dev.js",
    "lint": "eslint src --ext .js,.ts,.tsx --max-warnings 0 --cache",
    "check-type": "tsc --noEmit --skipLibCheck",
    "test": "jest --passWithNoTests",
    "test:all": "concurrently \"yarn test\"",
    "test:watch": "jest --passWithNoTests --watch",
    "test:coverage": "jest --collectCoverage --passWithNoTests",
    "prebuild": "yarn build:clean",
    "build": "cross-env stage=$stage webpack --config webpack.prod.js",
    "build:actions": "cross-env stage=dev1 webpack --config webpack.actions.js",
    "build:clean": "rimraf ./dist",
    "build:profile": "yarn build --profile",
    "postbuild": "node ./scripts/createEnvFile.js dev1 && react-snap && node ./scripts/removePrefetch.js && node ./scripts/removeSourceMap.js",
    "postinstall": "patch-package && yarn generate-apis",
    "generate-apis": "node ./scripts/generateApis.js",
    "redoc": "redoc-cli serve -p 30001",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix --max-warnings 0"
    ],
    "src/**/*.tsx": [
      "prettier --write",
      "eslint --fix --max-warnings 0",
      "yarn stylelint"
    ]
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@swc/cli": "^0.1.55",
    "@swc/jest": "^0.2.17",
    "@types/color": "^3.0.2",
    "@types/debug": "^4.1.5",
    "@types/history": "^4.7.3",
    "@types/jest": "^26.0.14",
    "@types/loadjs": "^3.5.0",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^5.0.11",
    "@types/react-router": "^5.1.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/styled-components": "^5.1.3",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "@typescript-eslint/types": "^5.10.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "jest": "^26.4.2",
    "lint-staged": "^11.1.1",
    "postcss": "^8.4.5",
    "postcss-styled": "^0.34.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "redoc-cli": "^0.9.2",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "dependencies": {
    "@meshkorea/cake-form": "3.1.5",
    "@meshkorea/vroong-design-system-web": "2.1.92-dependency-fix.2",
    "@openapitools/openapi-generator-cli": "^2.1.18",
    "@swc/core": "^1.2.133",
    "axios": "^0.20.0",
    "browserslist": "^4.19.1",
    "color": "^4.2.0",
    "concurrently": "^6.2.0",
    "copy-webpack-plugin": "^5.0.5",
    "core-js": "^3.6.0",
    "cross-env": "^6.0.3",
    "css-loader": "^6.5.1",
    "date-fns": "^2.16.1",
    "debug": "^4.1.1",
    "dotenv": "^14.2.0",
    "dotenv-webpack": "^7.0.3",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "loadjs": "^4.2.0",
    "lodash-es": "^4.17.15",
    "mobx": "^5.15.7",
    "mobx-react": "6.3.1",
    "mobx-react-router": "^4.1.0",
    "patch-package": "^6.2.0",
    "postinstall-postinstall": "^2.0.0",
    "react": "^17.0.2",
    "react-div-100vh": "^0.3.4",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.8.2",
    "react-responsive-picture": "^3.1.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scrolllock": "^5.0.0",
    "react-snap": "^1.23.0",
    "replace-in-file": "^4.1.3",
    "rimraf": "^3.0.0",
    "sanitize.css": "^12.0.1",
    "shelljs": "^0.8.3",
    "style-loader": "^3.3.1",
    "styled-components": "^5.2.0",
    "swc-loader": "^0.1.15",
    "ts-loader": "^6.2.1",
    "typescript": "^4.5.5",
    "url-search-params-polyfill": "^8.1.0",
    "webpack": "^5.67.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3",
    "webpack-merge": "^4.2.2"
  },
  "reactSnap": {
    "source": "dist",
    "publicPath": "/",
    "skipThirdPartyRequests": true,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ]
  },
  "resolutions": {
    "styled-components": "5.3.3",
    "react": "17.0.2",
    "@types/react": "^17.0.38",
    "@babel/parser": "7.16.10"
  }
}